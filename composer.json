{
    "name": "whitehatsllc/iseed",
    "description": "Generate a new Laravel database seed file based on data from the existing database table.",
    "keywords": ["laravel", "generators", "seed", "artisan"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "whitehatsllc",
            "email": "info@whitehats.ae"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "illuminate/filesystem": "5.x",
        "laravel/framework": "^5.3.8",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-0": {
            "Whitehatsllc\\Iseed": "src/"
        },
        "classmap": [
            "src/Whitehatsllc/Iseed/Exceptions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Whitehatsllc\\Iseed\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
